.protail{
  width: 700rpx;
  height: 350rpx;
  border-radius: 20rpx;
  background-image: linear-gradient(225deg, #3a3a41, #2c2b3d);
  border: solid 2px rgba(255, 255, 255, 0.5);
  margin: 0 auto 16rpx;
  box-shadow: 0rpx 10rpx 16rpx #ccc;
}
.tobox{
  width: 100%;
  height: 156rpx;
  position: relative;
}
.tobox .up{
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  /* border: solid 8rpx rgba(255, 255, 255, 0.1); */
  border: solid 8rpx red;
  display: inline-block;
  margin: auto;
  background: blue;
} 
.tobox .rt{
  position: absolute;
  top: 20rpx;
  right: 29rpx;
  width: 187rpx;
  display: inline-block;
  height: 21rpx;
} 
.tobox .rt .le{
  width: 11rpx;
  height: 21rpx;
  background-color: #ffffff;
  box-shadow: 0 0 2rpx 0 rgba(255, 255, 255, 0.5);
  float: left;
}
.tobox .rt .ri{
  width: 135rpx;
  height: 21rpx;
  font-family: DIN;
  font-size: 21rpx;
  font-weight: bold;
  font-stretch: condensed;
  line-height: 21rpx;
  letter-spacing: 5rpx;
  text-align: right;
  color: #ffffff;
  float: right;
  margin-right: 29rpx;
}
.protail .do{
  width: 100%;
  height: 48rpx;
  opacity: 0.9;
  font-family: PingFangSC;
  font-size: 34rpx;
  font-weight: 600;
  line-height: normal;
  letter-spacing: 2rpx;
  text-align: center;
  color: #ffffff;
}
.protail .md{
  display: flex;
  justify-content: space-around;
  flex-direction: row;
  text-align: center;
  margin-top: 21rpx;
}
.md .up{
  height: 28rpx;
  opacity: 0.7;
  font-family: PingFangSC;
  font-size: 20rpx;
  line-height: 28rpx;
  letter-spacing: 1px;
  color: #ffffff;
}
.md .do{
  height: 45rpx;
  font-family: DIN;
  font-size: 56rpx;
  font-weight: 600;
  line-height: 45rpx;
  letter-spacing: 2rpx;
  text-align: center;
  color: #ffffff;
  margin-top: 14rpx;
}
.md .le .do{
  font-size: 32rpx;
  font-family: PingFangSC;
}
.modu{
  width: 700rpx;
  height: 120rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid #eaeaea;
  background: greenyellow;
  line-height: 120rpx;
  text-indent: 25rpx;
  font-family: PingFangSC;
}
