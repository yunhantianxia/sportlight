/* pages/material/material.wxss */
.main{
  background: green;
  width: 100%;
  height: 735rpx;
}
.titup{
  text-align: center;
  font-family: PingFangSC;
  font-size: 20rpx;
  height: 28rpx;
  width: 100%;
  color: #2c2b3d;
  letter-spacing: 2rpx;
  line-height: 28rpx;
}
.countdown{
  height: 96rpx;
  font-family: DIN;
  font-size: 79rpx;
  font-weight: bold;
  font-stretch: condensed;
  letter-spacing: 3rpx;
  text-align: center;
  color: #2c2b3d;
  line-height: 96rpx;
}
.startmate{
  text-align: center;
  height: 100rpx;
  width: 600rpx;
  border-radius: 50rpx;
  background-color: #2c2b3d;
  margin: auto;
  color: #fff;
  line-height: 100rpx;
  font-family: PingFangSC;
  opacity: 0.9;
  letter-spacing: 3rpx;
  position: relative;
  margin-top: 36rpx;
}
.startmate span:nth-of-type(1){
  position: absolute;
  font-size: 32rpx;
  opacity: 0.1;
  right: 82rpx;
}
.triangle{
  height: 0;
  width: 0;
  /* background-color: #fff; */
  opacity: 0.2;
  border-top: 12.7rpx solid transparent;
  border-bottom: 12.7rpx solid transparent;
  border-left: 12.7rpx solid rgb(255, 255, 255);
  position: absolute;
  right: 56.7rpx;
  top:39rpx;
}
.titdo{
  height: 30rpx;
  opacity: 0.5;
  font-family: PingFangSC;
  font-size: 22rpx;
  letter-spacing: 2rpx;
  text-align: center;
  color: #2c2b3d;
  line-height: 30rpx;
  margin-top: 36rpx;
}